@model IEnumerable<string>
@{ Layout = null;}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>Language Features</title>
</head>
<body>
    <ul>
        @foreach (string str in Model) {
            <li>@str</li>
        }
    </ul>

</body>
</html>